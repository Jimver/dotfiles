# Increase backlight
"light -A 5"
   XF86MonBrightnessUp

# Decrease backlight
"light -U 5"
   XF86MonBrightnessDown
   
# Increase volume
"pulsemixer --change-volume +5"
  XF86AudioRaiseVolume
  
# Decrease volume
"pulsemixer --change-volume -5"
  XF86AudioLowerVolume
  
# Mute volume
"pulsemixer --toggle-mute"
  XF86AudioMute
  
# Play Pause key
"playerctl play-pause"
  XF86AudioPlay
  
# Next track
"playerctl next"
  XF86AudioNext
  
# Previous track
"playerctl previous"
  XF86AudioPrev

# Stop button
"playerctl stop"
  XF86AudioStop
